{
  "name": "flack",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "images:copy": "cp -r ./src/public/images ./dist/images",
    "server:watch": "npx nodemon ./server.js",
    "css:build": "npx postcss ./src/public/css/main.css -o ./dist/main.css",
    "css:watch": "npx postcss --watch ./src/public/css/main.css -o ./dist/main.css",
    "webpack:release": "npx webpack -p",
    "webpack:watch": "npx webpack --watch"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "3.0.1",
    "connect-mongo": "2.0.1",
    "elasticsearch": "15.2.0",
    "express": "4.16.3",
    "express-session": "1.15.6",
    "mongoose": "5.3.0",
    "socket.io": "2.1.1"
  },
  "devDependencies": {
    "html-webpack-plugin": "3.2.0",
    "jest": "23.6.0",
    "nodemon": "1.18.4",
    "postcss": "7.0.2",
    "postcss-cli": "6.0.0",
    "postcss-import": "12.0.0",
    "webpack": "4.19.1",
    "webpack-cli": "3.1.0"
  }
}
